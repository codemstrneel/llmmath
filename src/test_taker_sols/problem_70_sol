def solution(n: int) -> int:
    if n == 0 or n == 1:
        return 1
    motzkin = [0] * (n + 1)
    motzkin[0] = 1
    motzkin[1] = 1
    for i in range(2, n + 1):
        motzkin[i] = motzkin[i - 1]
        for k in range(i - 1):
            motzkin[i] += motzkin[k] * motzkin[i - 2 - k]
    return motzkin[n]