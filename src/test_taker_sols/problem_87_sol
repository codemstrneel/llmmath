def solution(input_string, number, low_range, high_range):
    if len(input_string) % 2 == 1:
        odd_factors = [i for i in range(1, number + 1) if number % i == 0 and i % 2 == 1]
        return sorted(odd_factors)
    else:
        ascii_sum = sum(ord(char) for char in input_string)
        if low_range <= ascii_sum <= high_range:
            return 20
        return ascii_sum