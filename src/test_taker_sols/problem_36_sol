def solution(int_list, char_string):
    char_count = char_string.count
    max_count = 0
    result = 0
    for num in int_list:
        current_count = char_count(str(num))
        if current_count > max_count:
            max_count = current_count
            result = num
    return int(''.join(map(str, int_list))) if max_count > 0 else 0