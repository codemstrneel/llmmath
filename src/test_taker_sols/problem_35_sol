def solution(int_list):
    from itertools import permutations
    
    def is_prime(n):
        if n <= 1:
            return False
        for i in range(2, int(n**0.5) + 1):
            if n % i == 0:
                return False
        return True
    
    for perm in permutations(int_list):
        num = int(''.join(map(str, perm)))
        if is_prime(num):
            return num
    return None